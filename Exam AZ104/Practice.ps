#Task 1- Use PS to deploy policy for Subscription and RG level.
https://docs.microsoft.com/en-us/azure/governance/policy/assign-policy-powershell 

# Create new RG 
Get-Help -name New-AzResourceGroup
New-AzResourceGroup -Name practicerg -Location eastus 
$rg = get-AzResourceGroup -name 'practicerg'
$definition = Get-AzPolicyDefinition | Where-Object { $_.Properties.DisplayName -eq "Azure Backup should be enabled for Virtual Machines" } # where-object mathces the policy with displayname
Write-Output $definition # display contents of $ variable   
#create new policy    
    New-AzPolicyAssignment -Name 'Azure Backup should be enabled for Virtual Machines' -DisplayName 'Audit VMs without managed disks Assignment' -Scope $rg.ResourceId -PolicyDefinition $definition   


